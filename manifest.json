{
    "name": "AniKeep",
    "version": "3.0",
    "description": "Add and manage all the anime you are currently watching!",
    "permissions": ["storage", "tabs", "storage"],
    "background": {
      "scripts": ["background.js", "js/utils.js", "js/api.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
}