{
    "name": "Anime Manager",
    "version": "1.0",
    "description": "Manage the anime you're currently watching on KissAnime!",
    "permissions": ["storage", "tabs", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
}